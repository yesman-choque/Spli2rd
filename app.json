{
  "expo": {
    "name": "Spli2rd",
    "slug": "Spli2rd",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#212121"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.yesman.spli2rd"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
